{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html",
      "has": [{ "type": "request_method", "value": "GET" }]
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "methods": ["POST", "GET", "PUT", "DELETE"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "continue": true
    }
  ]
}
